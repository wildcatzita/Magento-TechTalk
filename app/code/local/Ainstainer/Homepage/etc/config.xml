<?xml version="1.0"?>
<config>
    <modules>
        <Ainstainer_Homepage>
            <version>1.1</version>
        </Ainstainer_Homepage>
    </modules>
    <global>
        <blocks>
            <homepage>
                <class>Ainstainer_Homepage_Block</class>
            </homepage>
        </blocks>
        <models>
            <homepage>
                <class>Ainstainer_Homepage_Model</class>
                <resourceModel>ainstainer_homepage_resource</resourceModel>
            </homepage>
            <ainstainer_homepage_resource>
                <class>Ainstainer_Homepage_Model_Resource</class>
                <entities>
                    <homepage>
                        <table>homepage_banner</table>
                    </homepage>
                </entities>
            </ainstainer_homepage_resource>
        </models>
    </global>
    <frontend>
        <layout>
            <updates>
                <homepage>
                    <file>ainstainer/homepage.xml</file>
                </homepage>
            </updates>
        </layout>
    </frontend>
</config>

